{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
		<div class="container-fluid">
			<div class="pull-right">
				<a onclick="$('#import').submit();" class="btn btn-primary"><i class="fa fa-upload"></i> {{ button_import }}</a>
			</div>
			<h1>{{ heading_title1 }}</h1>
			<ul class="breadcrumb">
				{% for breadcrumb in breadcrumbs %}
				<li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
				{% endfor %}
			</ul>
		</div>
	</div>
	<div class="container-fluid">
		{% if error_warning %}
		<div class="alert alert-danger"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
		<button type="button" class="close" data-dismiss="alert">&times;</button>
		</div>
		{% endif %}
		{% if success %}
		<div class="alert alert-success"><i class="fa fa-check-circle"></i> {{ success }}
		<button type="button" class="close" data-dismiss="alert">&times;</button>
		</div>
		{% endif %}
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3 class="panel-title"><i class="fa fa-pencil"></i>  {{ heading_title }}</h3>
			</div>
			<div class="panel-body">
				<form action="{{ import }}" method="post" enctype="multipart/form-data" id="import">
					<div class="form-group">
						<label class="col-sm-3 control-label text-right">{{ entry_import }}</label>
						<div class="col-sm-9">
							<input type="file" name="import" class="form-control" />
						</div>
					</div>
				</form>
			</div>
		</div>
	</div>
</div>
{{ footer }}